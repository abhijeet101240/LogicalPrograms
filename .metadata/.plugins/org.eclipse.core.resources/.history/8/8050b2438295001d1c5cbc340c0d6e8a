package com.array;

import java.util.Scanner;

public class MaximumNumber {

	public static void getMaxNum(int a[]) {

		int n = a.length;
		int max = 0;

		for (int i = 0; i < n; i++) {
			
			if(max< a[i]) {
				max=a[i];
			}
		}
		System.out.println("The Maximum Element is"+ max);
	}

	public static void main(String[] args) {

		int a[] = { 9, 8, 4, 5, 10, 7 };

		MaximumNumber.getMaxNum(a);
	}
}
