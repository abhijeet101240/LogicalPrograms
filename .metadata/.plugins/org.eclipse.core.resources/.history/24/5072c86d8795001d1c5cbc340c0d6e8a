package com.string;

public class CountAlphabets {

	public static void getCountAlphabets(String str){
		
	int n=str.length();
	
	int count=0;
	
	for(int i=0;i<n;i++) {
	
		if(str.charAt(i)!=' ') {
			count++;
		}
		
		
	}
	System.out.println(count);
	
	}
	
	public static void main(String[] args) {
		String str="i am the best";
		getCountAlphabets(str);
	
	}
}
